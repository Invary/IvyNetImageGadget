    <system:String x:Key="donationQR1Label">BSC/Polygon/Etherium</system:String>
    <system:String x:Key="donationQR2Label">PayPay</system:String>
    <system:String x:Key="donationQR1Address">0xCbd4355d13CEA25D87F324E9f35A075adce6507c</system:String>
