    <BitmapImage x:Key="image_QR1" UriSource="image/donation/donation_bsc.png"/>
    <BitmapImage x:Key="image_QR2" UriSource="image/donation/paypayqr.png"/>
    <BitmapImage x:Key="image_Kofi" UriSource="image/donation/donation_kofi.png"/>

    <system:String x:Key="donationKofiTips" xml:space="preserve">ブラウザで ko-fi.com を開く</system:String>
    <system:String x:Key="donationDescription" xml:space="preserve">もしよろしければ寄付よろしくお願いします。&#13;Paypay、ko-fi、仮想通貨が利用可能です。</system:String>
