<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <system:String x:Key="windowTitle">Setting</system:String>

    <system:String x:Key="labelImageDirection">Image tiling direction</system:String>
    <system:String x:Key="imageDirectionVertical">Vertical</system:String>
    <system:String x:Key="imageDirectionHorizon">Horizon</system:String>

    <system:String x:Key="labelImage">Image item</system:String>

